<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.fje.clot.puzzle"
    android:versionCode="9"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="17" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:icon="@drawable/smalltiles"
        android:label="@string/app_name" >
        <activity
            android:name="edu.fje.clot.puzzle.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.fje.clot.puzzle.GameActivity" />

        <service android:name="edu.fje.clot.puzzle.service.audio.MusicService" />
        <service android:name="edu.fje.clot.puzzle.service.image.ImageService" />
    </application>

</manifest>